<script>
	$("#file").change(function(){
		var file = $("#file")[0].files[0];
		alert(file);
		if (confirm("업로드 ㄱ?")) {
			return;
		} else {
			alert("!");
			$.ajax({
				type : "post",
				url : "ajaxUpload",
				data : {},
				dataType : "json",
				success : function(data) {
					alert(data + " 업로드 완료");
				}
			});
		}
	});
</script>
